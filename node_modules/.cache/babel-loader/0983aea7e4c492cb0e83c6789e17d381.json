: {\n    isUpdating: function isUpdating(val) {\n      var _this = this;\n\n      if (val) {\n        setTimeout(function () {\n          return _this.isUpdating = false;\n        }, 3000);\n      }\n    }\n  },\n  methods: {\n    remove: function remove(item) {\n      var index = this.friends.indexOf(item.name);\n      if (index >= 0) this.friends.splice(index, 1);\n    }\n  }\n};","map":{"version":3,"sources":["app.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0nBA,eAAA;AACA,EAAA,IADA,kBACA;AACA,QAAA,IAAA,GAAA;AACA,SAAA,8CADA;AAEA,SAAA,8CAFA;AAGA,SAAA,8CAHA;AAIA,SAAA,8CAJA;AAKA,SAAA;AALA,KAAA;AAQA,WAAA;AACA,MAAA,UAAA,EAAA,IADA;AAEA,MAAA,OAAA,EAAA,CAAA,cAAA,EAAA,aAAA,CAFA;AAGA,MAAA,UAAA,EAAA,KAHA;AAIA,MAAA,IAAA,EAAA,eAJA;AAKA,MAAA,MAAA,EAAA,CACA;AAAA,QAAA,MAAA,EAAA;AAAA,OADA,EAEA;AAAA,QAAA,IAAA,EAAA,cAAA;AAAA,QAAA,KAAA,EAAA,SAAA;AAAA,QAAA,MAAA,EAAA,IAAA,CAAA,CAAA;AAAA,OAFA,EAGA;AAAA,QAAA,IAAA,EAAA,aAAA;AAAA,QAAA,KAAA,EAAA,SAAA;AAAA,QAAA,MAAA,EAAA,IAAA,CAAA,CAAA;AAAA,OAHA,EAIA;AAAA,QAAA,IAAA,EAAA,eAAA;AAAA,QAAA,KAAA,EAAA,SAAA;AAAA,QAAA,MAAA,EAAA,IAAA,CAAA,CAAA;AAAA,OAJA,EAKA;AAAA,QAAA,IAAA,EAAA,cAAA;AAAA,QAAA,KAAA,EAAA,SAAA;AAAA,QAAA,MAAA,EAAA,IAAA,CAAA,CAAA;AAAA,OALA,EAMA;AAAA,QAAA,OAAA,EAAA;AAAA,OANA,EAOA;AAAA,QAAA,MAAA,EAAA;AAAA,OAPA,EAQA;AAAA,QAAA,IAAA,EAAA,aAAA;AAAA,QAAA,KAAA,EAAA,SAAA;AAAA,QAAA,MAAA,EAAA,IAAA,CAAA,CAAA;AAAA,OARA,EASA;AAAA,QAAA,IAAA,EAAA,aAAA;AAAA,QAAA,KAAA,EAAA,SAAA;AAAA,QAAA,MAAA,EAAA,IAAA,CAAA,CAAA;AAAA,OATA,EAUA;AAAA,QAAA,IAAA,EAAA,YAAA;AAAA,QAAA,KAAA,EAAA,SAAA;AAAA,QAAA,MAAA,EAAA,IAAA,CAAA,CAAA;AAAA,OAVA,EAWA;AAAA,QAAA,IAAA,EAAA,iBAAA;AAAA,QAAA,KAAA,EAAA,SAAA;AAAA,QAAA,MAAA,EAAA,IAAA,CAAA,CAAA;AAAA,OAXA,CALA;AAkBA,MAAA,KAAA,EAAA;AAlBA,KAAA;AAoBA,GA9BA;AAgCA,EAAA,KAAA,EAAA;AACA,IAAA,UADA,sBACA,GADA,EACA;AAAA;;AACA,UAAA,GAAA,EAAA;AACA,QAAA,UAAA,CAAA;AAAA,iBAAA,KAAA,CAAA,UAAA,GAAA,KAAA;AAAA,SAAA,EAAA,IAAA,CAAA;AACA;AACA;AALA,GAhCA;AAwCA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,kBACA,IADA,EACA;AACA,UAAA,KAAA,GAAA,KAAA,OAAA,CAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA;AACA,UAAA,KAAA,IAAA,CAAA,EAAA,KAAA,OAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA;AAJA;AAxCA,CAAA","sourcesContent":["<template>\n <!-- inspireでホームCalのCSSを制御 -->\n  <v-app id=\"inspire\">\n    <v-sheet height=\"64\">\n      <v-toolbar\n        flat\n      >\n        <v-app-bar-nav-icon\n        @click.stop=\"drawer = !drawer\"\n        class=\"ml-auto\"\n        >\n        </v-app-bar-nav-icon> \n\n            <v-btn\n              icon\n              class=\"ml-auto\"\n              @click=\"$refs.calendar.prev()\"\n            >\n              <v-icon>mdi-chevron-left</v-icon>\n            </v-btn>\n\n            <v-toolbar-title v-if=\"$refs.calendar\">\n              {{ $refs.calendar.title }}\n            </v-toolbar-title>\n\n            <v-btn\n              icon\n              class=\"mr-auto\"\n              @click=\"$refs.calendar.next()\"\n            >\n              <v-icon>mdi-chevron-right</v-icon>\n            </v-btn>\n\n            <v-btn\n              outlined\n              x-small\n              class=\"mr-auto\"\n              @click=\"setToday\"\n            >\n              Today\n            </v-btn>\n      </v-toolbar>\n    </v-sheet>\n\n    <v-navigation-drawer\n      v-model=\"drawer\"\n      absolute\n      left\n      temporary\n    >\n      <v-list\n        nav\n        dense\n      >\n        <v-list-item-group\n          v-model=\"group\"\n          active-class=\"deep-purple--text text--accent-4\"\n        >\n          <h5>　表示の種類</h5>\n          <v-list-item @click=\"type = 'day'\">\n          <v-icon>mdi-view-day-outline</v-icon>\n            <v-list-item-title>　day</v-list-item-title>\n          </v-list-item>\n\n          <v-list-item @click=\"type = 'week'\">\n          <v-icon>mdi-view-week-outline</v-icon>\n            <v-list-item-title>　week</v-list-item-title>\n          </v-list-item>\n\n          <v-list-item @click=\"type = 'month'\">\n          <v-icon>mdi-grid</v-icon>\n            <v-list-item-title>　month</v-list-item-title>\n          </v-list-item>\n\n          <v-list-item>\n          <v-icon>mdi-account-group</v-icon>\n            <v-list-item-title>　gloup</v-list-item-title>\n          </v-list-item>\n\n          <v-list-item>\n          <v-icon>mdi-account</v-icon>\n            <v-list-item-title>　person</v-list-item-title>\n          </v-list-item>\n\n        </v-list-item-group>\n      </v-list>\n    </v-navigation-drawer>\n\n    <v-sheet height=\"600\">\n      <v-calendar\n        ref=\"calendar\"\n        v-model=\"focus\"\n        color=\"red lighten-2\"\n        :events=\"events\"\n        :event-color=\"getEventColor\"\n        :type=\"type\"\n        @click:event=\"showEvent\"\n        @click:more=\"viewDay\"\n        @click:date=\"viewDay\"\n        @change=\"updateRange\"\n      >\n      </v-calendar>\n    </v-sheet>\n\n   <!-- +アイコンの登録ボタン -->\n    <v-sheet height=\"300\">\n      <v-row justify=\"center\">\n        <v-dialog\n          v-model=\"dialog\"\n          fullscreen\n          hide-overlay\n          transition=\"dialog-bottom-transition\"\n        >\n          <template v-slot:activator=\"{ on, attrs }\">\n            <v-btn\n              elevation=\"6\"\n              color=\"grey darken\"\n              fab\n              dark\n              v-bind=\"attrs\"\n              v-on=\"on\"\n            >\n              <v-icon dark>\n                mdi-plus\n              </v-icon>\n            </v-btn>\n          </template>\n\n         <!-- ダイアログ登録フォーム -->\n          <v-card>\n            <v-toolbar\n              dark\n              color=\"grey darken\"\n            >\n              <v-btn\n                icon\n                dark\n                @click=\"dialog = false\"\n              >\n                <v-icon>mdi-close</v-icon>\n              </v-btn>\n              <v-spacer></v-spacer>\n              <v-toolbar-facility>\n                <v-btn\n                  dark\n                  text\n                  @click=\"dialog = false\"\n                >\n                  Save\n                </v-btn>\n              </v-toolbar-facility>\n            </v-toolbar>\n    \n            <v-list-item>\n              <v-col\n                cols=\"12\"\n                sm=\"6\"\n                md=\"5\"\n              >\n                <v-text-field\n                  label=\"タイトル\"\n                ></v-text-field>\n              </v-col>\n            </v-list-item>\n    \n\n            <v-container fluid>\n              <v-row>\n                <v-col\n                  class=\"d-flex\"\n                  cols=\"12\"\n                  sm=\"3\"\n                >\n                  <v-select\n                    :items=\"states\"\n                    label=\"予定の種類\"\n                    outlined\n                  ></v-select>\n                </v-col>\n          \n                <v-col\n                  class=\"d-flex\"\n                  cols=\"12\"\n                  sm=\"3\"\n                >\n                  <v-select\n                    :items=\"facility\"\n                    item-text=\"state\"\n                    item-value=\"abbr\"\n                    label=\"設備を選ぶ\"\n                    outlined\n                  ></v-select>\n                </v-col>\n              </v-row>\n            </v-container>\n\n\n         <!-- ダイアログ内カレンダー・タイム -->\n            <v-container fluid>\n              <v-row>\n                <v-col\n                  cols=\"12\"\n                  sm=\"6\"\n                  md=\"4\"\n                >\n    \n                  <datetime \n                    type=\"datetime\"\n                    v-model=\"start_time\" placeholder=\"Start Time\"\n                    :minute-step=\"15\"\n                    auto \n                  ></datetime>\n    \n                </v-col>\n              </v-row>\n    \n              <v-row>\n                <v-col\n                  cols=\"12\"\n                  sm=\"6\"\n                  md=\"4\"\n                >\n    \n                  <datetime \n                    type=\"datetime\"\n                    v-model=\"end_time\" placeholder=\"End Time\"\n                    :minute-step=\"15\"\n                    auto \n                  ></datetime>\n    \n                </v-col>\n              </v-row>\n            </v-container>\n\n           <!-- ダイアログ場所・メモの入力 -->\n          <v-container fluid>\n            <v-row>\n              <v-col\n                cols=\"12\"\n                md=\"6\"\n              >\n                <v-textarea\n                  outlined\n                  name=\"input-7-4\"\n                  label=\"場所・メモ\"\n                ></v-textarea>\n              </v-col>\n            </v-row>\n          </v-container>\n\n           <!-- ダイアログ参加者の追加 -->\n  <v-container fluid>\n    <v-row align=\"center\">\n      <v-col\n      cols=\"12\"\n      md=\"6\"\n      >\n        <v-autocomplete\n          v-model=\"selectedMember1\"\n          :items=\"member1\"\n          chips\n          clearable\n          color=\"grey\"\n          label=\"参加者を追加\"\n          item-text=\"name\"\n          item-value=\"group\"\n          multiple\n        >\n          <template v-slot:selection=\"data\">\n            <v-chip\n              v-bind=\"data.attrs\"\n              :input-value=\"data.selected\"\n              close\n              @click=\"data.select\"\n              @click:close=\"remove(data.item)\"\n            >\n              {{ data.item.name }}\n            </v-chip>\n          </template>\n        </v-autocomplete>\n      </v-col>\n    </v-row>\n  </v-container>\n\n<template>\n  <v-card\n    color=\"blue-grey darken-1\"\n    dark\n    :loading=\"isUpdating\"\n  >\n    <template v-slot:progress>\n      <v-progress-linear\n        absolute\n        color=\"green lighten-3\"\n        height=\"4\"\n        indeterminate\n      ></v-progress-linear>\n    </template>\n    <v-img\n      height=\"200\"\n      src=\"https://cdn.vuetifyjs.com/images/cards/dark-beach.jpg\"\n    >\n      <v-row>\n        <v-col\n          class=\"text-right\"\n          cols=\"12\"\n        >\n          <v-menu\n            bottom\n            left\n            transition=\"slide-y-transition\"\n          >\n            <template v-slot:activator=\"{ on, attrs }\">\n              <v-btn\n                icon\n                v-bind=\"attrs\"\n                v-on=\"on\"\n              >\n                <v-icon>mdi-dots-vertical</v-icon>\n              </v-btn>\n            </template>\n            <v-list>\n              <v-list-item @click=\"isUpdating = true\">\n                <v-list-item-action>\n                  <v-icon>mdi-cog</v-icon>\n                </v-list-item-action>\n                <v-list-item-content>\n                  <v-list-item-title>Update</v-list-item-title>\n                </v-list-item-content>\n              </v-list-item>\n            </v-list>\n          </v-menu>\n        </v-col>\n        <v-row\n          class=\"pa-4\"\n          align=\"center\"\n          justify=\"center\"\n        >\n          <v-col class=\"text-center\">\n            <h3 class=\"text-h5\">\n              {{ name }}\n            </h3>\n            <span class=\"grey--text text--lighten-1\">{{ title }}</span>\n          </v-col>\n        </v-row>\n      </v-row>\n    </v-img>\n    <v-form>\n      <v-container>\n        <v-row>\n          <v-col\n            cols=\"12\"\n            md=\"6\"\n          >\n            <v-text-field\n              v-model=\"name\"\n              :disabled=\"isUpdating\"\n              filled\n              color=\"blue-grey lighten-2\"\n              label=\"Name\"\n            ></v-text-field>\n          </v-col>\n          <v-col\n            cols=\"12\"\n            md=\"6\"\n          >\n            <v-text-field\n              v-model=\"title\"\n              :disabled=\"isUpdating\"\n              filled\n              color=\"blue-grey lighten-2\"\n              label=\"Title\"\n            ></v-text-field>\n          </v-col>\n          <v-col cols=\"12\">\n            <v-autocomplete\n              v-model=\"friends\"\n              :disabled=\"isUpdating\"\n              :items=\"people\"\n              filled\n              chips\n              color=\"blue-grey lighten-2\"\n              label=\"Select\"\n              item-text=\"name\"\n              item-value=\"name\"\n              multiple\n            >\n              <template v-slot:selection=\"data\">\n                <v-chip\n                  v-bind=\"data.attrs\"\n                  :input-value=\"data.selected\"\n                  close\n                  @click=\"data.select\"\n                  @click:close=\"remove(data.item)\"\n                >\n                  <v-avatar left>\n                    <v-img :src=\"data.item.avatar\"></v-img>\n                  </v-avatar>\n                  {{ data.item.name }}\n                </v-chip>\n              </template>\n              <template v-slot:item=\"data\">\n                <template v-if=\"typeof data.item !== 'object'\">\n                  <v-list-item-content v-text=\"data.item\"></v-list-item-content>\n                </template>\n                <template v-else>\n                  <v-list-item-avatar>\n                    <img :src=\"data.item.avatar\">\n                  </v-list-item-avatar>\n                  <v-list-item-content>\n                    <v-list-item-title v-html=\"data.item.name\"></v-list-item-title>\n                    <v-list-item-subtitle v-html=\"data.item.group\"></v-list-item-subtitle>\n                  </v-list-item-content>\n                </template>\n              </template>\n            </v-autocomplete>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-form>\n    <v-divider></v-divider>\n    <v-card-actions>\n      <v-switch\n        v-model=\"autoUpdate\"\n        :disabled=\"isUpdating\"\n        class=\"mt-0\"\n        color=\"green lighten-2\"\n        hide-details\n        label=\"Auto Update\"\n      ></v-switch>\n      <v-spacer></v-spacer>\n      <v-btn\n        :disabled=\"autoUpdate\"\n        :loading=\"isUpdating\"\n        color=\"blue-grey darken-3\"\n        depressed\n        @click=\"isUpdating = true\"\n      >\n        <v-icon left>\n          mdi-update\n        </v-icon>\n        Update Now\n      </v-btn>\n    </v-card-actions>\n  </v-card>\n</template>\n\n          </v-card>\n        </v-dialog>\n      </v-row>\n    </v-sheet>\n  </v-app>\n</template>\n\n\n<style scoped>\n    .my-time-picker >>> .vue__time-picker .dropdown ul li:not([disabled]).active,\n    .my-time-picker >>> .vue__time-picker .dropdown ul li:not([disabled]).active:focus,\n    .my-time-picker >>> .vue__time-picker .dropdown ul li:not([disabled]).active:hover {\n        background: #999;\n    }\n</style>\n\n\n<script>\nimport VueTimepicker from 'vue2-timepicker'\nimport 'vue2-timepicker/dist/VueTimepicker.css'\n\nimport { Datetime } from 'vue-datetime'\nimport 'vue-datetime/dist/vue-datetime.css'\n\n  export default {\n  components: {\n    'vue-timepicker': VueTimepicker,\n    'datetime' : Datetime,\n  },\n    data: () => ({\n      menu:[],\n      menu2: [],\n      dialog: null,\n      dialog2: false,\n      date:'',\n      time:'',\n      datetime:'',\n      notifications: [],\n      drawer: false,\n      group: null,\n      focus: '',\n      type: '