{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport { Datetime } from 'vue-datetime';\nimport 'vue-datetime/dist/vue-datetime.css';\nexport default {\n  components: {\n    datetime: Datetime\n  }\n};","map":{"version":3,"sources":["app.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoRA,SAAA,QAAA,QAAA,cAAA;AACA,OAAA,oCAAA;AAEA,eAAA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,QAAA,EAAA;AADA;AAFA,CAAA","sourcesContent":["\n<template>\n\n<div>\n<!-- inspireでホームCalのCSSを制御 -->\n  <v-app id=\"inspire\">\n\n    <v-sheet height=\"64\">\n      <v-toolbar\n        flat\n      >\n        <v-app-bar-nav-icon\n        @click.stop=\"drawer = !drawer\"\n        class=\"ml-auto\"\n        >\n        </v-app-bar-nav-icon> \n\n            <v-btn\n              icon\n              class=\"ml-auto\"\n              @click=\"$refs.calendar.prev()\"\n            >\n              <v-icon>mdi-chevron-left</v-icon>\n            </v-btn>\n\n            <v-toolbar-title v-if=\"$refs.calendar\">\n              {{ $refs.calendar.title }}\n            </v-toolbar-title>\n\n            <v-btn\n              icon\n              class=\"mr-auto\"\n              @click=\"$refs.calendar.next()\"\n            >\n              <v-icon>mdi-chevron-right</v-icon>\n            </v-btn>\n\n            <v-btn\n              outlined\n              x-small\n              class=\"mr-auto\"\n              @click=\"setToday\"\n            >\n              Today\n            </v-btn>\n      </v-toolbar>\n    </v-sheet>\n\n      <v-navigation-drawer\n        v-model=\"drawer\"\n        absolute\n        left\n        temporary\n      >\n        <v-list\n          nav\n          dense\n        >\n          <v-list-item-group\n            v-model=\"group\"\n            active-class=\"deep-purple--text text--accent-4\"\n          >\n            <h5>　表示の種類</h5>\n            <v-list-item @click=\"type = 'day'\">\n            <v-icon>mdi-view-day-outline</v-icon>\n              <v-list-item-title>　day</v-list-item-title>\n            </v-list-item>\n  \n            <v-list-item @click=\"type = 'week'\">\n            <v-icon>mdi-view-week-outline</v-icon>\n              <v-list-item-title>　week</v-list-item-title>\n            </v-list-item>\n  \n            <v-list-item @click=\"type = 'month'\">\n            <v-icon>mdi-grid</v-icon>\n              <v-list-item-title>　month</v-list-item-title>\n            </v-list-item>\n  \n            <v-list-item>\n            <v-icon>mdi-account-group</v-icon>\n              <v-list-item-title>　gloup</v-list-item-title>\n            </v-list-item>\n  \n            <v-list-item>\n            <v-icon>mdi-account</v-icon>\n              <v-list-item-title>　person</v-list-item-title>\n            </v-list-item>\n\n          </v-list-item-group>\n        </v-list>\n      </v-navigation-drawer>\n\n        <v-sheet height=\"600\">\n          <v-calendar\n            ref=\"calendar\"\n            v-model=\"focus\"\n            color=\"red lighten-2\"\n            :events=\"events\"\n            :event-color=\"getEventColor\"\n            :type=\"type\"\n            @click:event=\"showEvent\"\n            @click:more=\"viewDay\"\n            @click:date=\"viewDay\"\n            @change=\"updateRange\"\n          >\n          </v-calendar>\n        </v-sheet>\n\n  <!-- +アイコンの登録ボタン -->\n  <v-sheet height=\"300\">\n  <v-row justify=\"center\">\n    <v-dialog\n      v-model=\"dialog\"\n      fullscreen\n      hide-overlay\n      transition=\"dialog-bottom-transition\"\n    >\n      <template v-slot:activator=\"{ on, attrs }\">\n\n        <v-btn\n          elevation=\"6\"\n          color=\"grey darken\"\n          fab\n          dark\n          v-bind=\"attrs\"\n          v-on=\"on\"\n        >\n          <v-icon dark>\n            mdi-plus\n          </v-icon>\n        </v-btn>\n\n        <!-- ダイアログ登録フォーム -->\n      </template>\n      <v-card>\n        <v-toolbar\n          dark\n          color=\"grey darken\"\n        >\n          <v-btn\n            icon\n            dark\n            @click=\"dialog = false\"\n          >\n            <v-icon>mdi-close</v-icon>\n          </v-btn>\n          <v-spacer></v-spacer>\n          <v-toolbar-items>\n            <v-btn\n              dark\n              text\n              @click=\"dialog = false\"\n            >\n              Save\n            </v-btn>\n          </v-toolbar-items>\n        </v-toolbar>\n\n        <v-list-item>\n          <v-col\n            cols=\"12\"\n            sm=\"6\"\n            md=\"5\"\n          >\n            <v-text-field\n              label=\"タイトル\"\n            ></v-text-field>\n          </v-col>\n        </v-list-item>\n\n      <template>\n        <v-container fluid>\n          <v-row>\n            <v-col\n              class=\"d-flex\"\n              cols=\"12\"\n              sm=\"3\"\n            >\n              <v-select\n                :items=\"states\"\n                label=\"予定の種類\"\n                outlined\n              ></v-select>\n            </v-col>\n      \n            <v-col\n              class=\"d-flex\"\n              cols=\"12\"\n              sm=\"3\"\n            >\n              <v-select\n                :items=\"items\"\n                item-text=\"state\"\n                item-value=\"abbr\"\n                label=\"設備を選ぶ\"\n                outlined\n              ></v-select>\n            </v-col>\n          </v-row>\n        </v-container>\n      </template>\n\n          <!-- <v-list-item>\n              <v-checkbox v-model=\"notifications\"></v-checkbox>\n              <v-list-item-title >終日</v-list-item-title>\n          </v-list-item> -->\n          \n          <!-- ダイアログ内カレンダー -->\n\n      <template>\n        <v-row>\n          <v-col\n            cols=\"12\"\n            sm=\"6\"\n            md=\"4\"\n          >\n            <v-menu\n              ref=\"menu\"\n              v-model=\"menu\"\n              :close-on-content-click=\"false\"\n              :return-value.sync=\"date\"\n              transition=\"scale-transition\"\n              offset-y\n              min-width=\"auto\"\n            >\n              <template v-slot:activator=\"{ on, attrs }\">\n                <v-text-field\n                  v-model=\"date\"\n                  label=\"Picker in menu\"\n                  prepend-icon=\"mdi-calendar\"\n                  readonly\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                ></v-text-field>\n              </template>\n              <v-date-picker\n                v-model=\"date\"\n                no-title\n                scrollable\n              >\n                <v-spacer></v-spacer>\n                <v-btn\n                  text\n                  color=\"primary\"\n                  @click=\"menu = false\"\n                >\n                  Cancel\n                </v-btn>\n                <v-btn\n                  text\n                  color=\"primary\"\n                  @click=\"$refs.menu.save(date)\"\n                >\n                  OK\n                </v-btn>\n              </v-date-picker>\n            </v-menu>\n          </v-col>\n          <v-spacer></v-spacer>\n        </v-row>\n      </template>\n\n <datetime v-model='datetime'></datetime>\n\n      </v-card>\n    </v-dialog>\n  </v-row>\n\n    </v-sheet>\n  </v-app>\n</div>\n\n</template>\n\n<script>\n\nimport { Datetime } from 'vue-datetime'\nimport 'vue-datetime/dist/vue-datetime.css'\n\n    export default {\n\n        components: {\n            datetime : Datetime\n        },\n\n    }\n\n</script>"],"sourceRoot":"app/javascript"},"metadata":{},"sourceType":"module"}