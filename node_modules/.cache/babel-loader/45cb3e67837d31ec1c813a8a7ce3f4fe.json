n            >\n              Today\n            </v-btn>\n      </v-toolbar>\n    </v-sheet>\n\n    <v-navigation-drawer\n      v-model=\"drawer\"\n      absolute\n      left\n      temporary\n    >\n      <v-list\n        nav\n        dense\n      >\n        <v-list-item-group\n          v-model=\"group\"\n          active-class=\"deep-purple--text text--accent-4\"\n        >\n          <h5>　表示の種類</h5>\n          <v-list-item @click=\"type = 'day'\">\n          <v-icon>mdi-view-day-outline</v-icon>\n            <v-list-item-title>　day</v-list-item-title>\n          </v-list-item>\n\n          <v-list-item @click=\"type = 'week'\">\n          <v-icon>mdi-view-week-outline</v-icon>\n            <v-list-item-title>　week</v-list-item-title>\n          </v-list-item>\n\n          <v-list-item @click=\"type = 'month'\">\n          <v-icon>mdi-grid</v-icon>\n            <v-list-item-title>　month</v-list-item-title>\n          </v-list-item>\n\n          <v-list-item>\n          <v-icon>mdi-account-group</v-icon>\n            <v-list-item-title>　gloup</v-list-item-title>\n          </v-list-item>\n\n          <v-list-item>\n          <v-icon>mdi-account</v-icon>\n            <v-list-item-title>　person</v-list-item-title>\n          </v-list-item>\n\n        </v-list-item-group>\n      </v-list>\n    </v-navigation-drawer>\n\n    <v-sheet height=\"600\">\n      <v-calendar\n        ref=\"calendar\"\n        v-model=\"focus\"\n        color=\"red lighten-2\"\n        :events=\"events\"\n        :event-color=\"getEventColor\"\n        :type=\"type\"\n        @click:event=\"showEvent\"\n        @click:more=\"viewDay\"\n        @click:date=\"viewDay\"\n        @change=\"updateRange\"\n      >\n      </v-calendar>\n    </v-sheet>\n\n   <!-- +アイコンの登録ボタン -->\n    <v-sheet height=\"300\">\n      <v-row justify=\"center\">\n        <v-dialog\n          v-model=\"dialog\"\n          fullscreen\n          hide-overlay\n          transition=\"dialog-bottom-transition\"\n        >\n          <template v-slot:activator=\"{ on, attrs }\">\n            <v-btn\n              elevation=\"6\"\n              color=\"grey darken\"\n              fab\n              dark\n              v-bind=\"attrs\"\n              v-on=\"on\"\n            >\n              <v-icon dark>\n                mdi-plus\n              </v-icon>\n            </v-btn>\n          </template>\n\n         <!-- ダイアログ登録フォーム -->\n          <v-card>\n            <v-toolbar\n              dark\n              color=\"grey darken\"\n            >\n              <v-btn\n                icon\n                dark\n                @click=\"dialog = false\"\n              >\n                <v-icon>mdi-close</v-icon>\n              </v-btn>\n              <v-spacer></v-spacer>\n              <v-toolbar-facility>\n                <v-btn\n                  dark\n                  text\n                  @click=\"dialog = false\"\n                >\n                  Save\n                </v-btn>\n              </v-toolbar-facility>\n            </v-toolbar>\n    \n            <v-list-item>\n              <v-col\n                cols=\"12\"\n                sm=\"6\"\n                md=\"5\"\n              >\n                <v-text-field\n                  label=\"タイトル\"\n                ></v-text-field>\n              </v-col>\n            </v-list-item>\n    \n\n            <v-container fluid>\n              <v-row>\n                <v-col\n                  class=\"d-flex\"\n                  cols=\"12\"\n                  sm=\"3\"\n                >\n                  <v-select\n                    :items=\"states\"\n                    label=\"予定の種類\"\n                    outlined\n                  ></v-select>\n                </v-col>\n          \n                <v-col\n                  class=\"d-flex\"\n                  cols=\"12\"\n                  sm=\"3\"\n                >\n                  <v-select\n                    :items=\"facility\"\n                    item-text=\"state\"\n                    item-value=\"abbr\"\n                    label=\"設備を選ぶ\"\n                    outlined\n                  ></v-select>\n                </v-col>\n              </v-row>\n            </v-container>\n\n\n         <!-- ダイアログ内カレンダー -->\n            <v-container fluid>\n              <v-row>\n                <v-col\n                  cols=\"12\"\n                  sm=\"6\"\n                  md=\"4\"\n                >\n    \n                  <datetime \n                    type=\"datetime\"\n                    v-model=\"start_time\" placeholder=\"Start Time\"\n                    :minute-step=\"15\"\n                    auto \n                  ></datetime>\n    \n                </v-col>\n              </v-row>\n    \n              <v-row>\n                <v-col\n                  cols=\"12\"\n                  sm=\"6\"\n                  md=\"4\"\n                >\n    \n                  <datetime \n                    type=\"datetime\"\n                    v-model=\"end_time\" placeholder=\"End Time\"\n                    :minute-step=\"15\"\n                    auto \n                  ></datetime>\n    \n                </v-col>\n              </v-row>\n            </v-container>\n\n           <!-- ダイアログ場所・メモの入力 -->\n          <v-container fluid>\n            <v-row>\n              <v-col\n                cols=\"12\"\n                md=\"6\"\n              >\n                <v-textarea\n                  outlined\n                  name=\"input-7-4\"\n                  label=\"場所・メモ\"\n                ></v-textarea>\n              </v-col>\n            </v-row>\n          </v-container>\n\n           <!-- ダイアログ参加者の追加 -->\n\n            <v-row>\n                <v-dialog\n                  v-model=\"dialog2\"\n                  persistent\n                  max-width=\"600px\"\n                >\n                  <template v-slot:activator=\"{ on, attrs }\">\n                    <v-btn\n                      color=\"grey\"\n                      dark\n                      v-bind=\"attrs\"\n                      v-on=\"on\"\n                    >\n                      参加者を変更\n                    </v-btn>\n                  </template>\n            \n                  <v-card>\n                    <v-card-title>\n                      <span class=\"text-h5\">参加者を追加</span>\n                    </v-card-title>\n            \n            \n                <v-container fluid>\n                  <v-row>\n                    <v-col cols=\"12\">\n                    <v-select\n                      v-model=\"selectedMember1\"\n                      :items=\"member1\"\n                      label=\"Company1\"\n                      multiple\n                      chips\n                    >\n                    <template v-slot:prepend-item>\n                      <v-list-item\n                        ripple\n                        @click=\"toggle1\"\n                      >\n                        <v-list-item-action>\n                          <v-icon :color=\"selectedMember1.length > 0 ? 'indigo darken-4' : ''\">\n                            {{ icon }}\n                          </v-icon>\n                        </v-list-item-action>\n                        <v-list-item-content>\n                          <v-list-item-title>\n                            Select All\n                          </v-list-item-title>\n                        </v-list-item-content>\n                      </v-list-item>\n                      <v-divider class=\"mt-2\"></v-divider>\n                    </template>\n                    </v-select>\n                    </v-col>\n                  </v-row>\n                </v-container>\n                <v-container fluid>\n                  <v-row>\n                    <v-col cols=\"12\">\n                    <v-select\n                      v-model=\"selectedMember2\"\n                      :items=\"member2\"\n                      label=\"Company2\"\n                      multiple\n                      chips\n                    >\n                    <template v-slot:prepend-item>\n                      <v-list-item\n                        ripple\n                        @click=\"toggle2\"\n                      >\n                        <v-list-item-action>\n                          <v-icon :color=\"selectedMember2.length > 0 ? 'indigo darken-4' : ''\">\n                            {{ icon }}\n                          </v-icon>\n                        </v-list-item-action>\n                        <v-list-item-content>\n                          <v-list-item-title>\n                            Select All\n                          </v-list-item-title>\n                        </v-list-item-content>\n                      </v-list-item>\n                      <v-divider class=\"mt-2\"></v-divider>\n                    </template>\n                    </v-select>\n                    </v-col>\n                  </v-row>\n                </v-container>\n                <v-container fluid>\n                  <v-row>\n                    <v-col cols=\"12\">\n                    <v-select\n                      v-model=\"selectedMember3\"\n                      :items=\"member3\"\n                      label=\"Company3\"\n                      multiple\n                      chips\n                    >\n                    <template v-slot:prepend-item>\n                      <v-list-item\n                        ripple\n                        @click=\"toggle3\"\n                      >\n                        <v-list-item-action>\n                          <v-icon :color=\"selectedMember3.length > 0 ? 'indigo darken-4' : ''\">\n                            {{ icon }}\n                          </v-icon>\n                        </v-list-item-action>\n                        <v-list-item-content>\n                          <v-list-item-title>\n                            Select All\n                          </v-list-item-title>\n                        </v-list-item-content>\n                      </v-list-item>\n                      <v-divider class=\"mt-2\"></v-divider>\n                    </template>\n                    </v-select>\n                    </v-col>\n                  </v-row>\n                </v-container>\n            \n                    <v-card-actions>\n                      <v-spacer></v-spacer>\n                      <v-btn\n                        color=\"blue darken-1\"\n                        text\n                        @click=\"dialog2 = false\"\n                      >\n                        Close\n                      </v-btn>\n                      <v-btn\n                        color=\"blue darken-1\"\n                        text\n                        @click=\"dialog2 = false\"\n                      >\n                        Save\n                      </v-btn>\n                    </v-card-actions>\n                  </v-card>\n                </v-dialog>\n            </v-row>\n\n  <v-container fluid>\n    <v-row align=\"center\">\n                  <v-col\n                  cols=\"12\"\n                  md=\"6\"\n                  >\n                    <v-select\n                      v-model=\"selectedMember1\"\n                      :items=\"member1\"\n                      multiple\n                      chips\n                    >\n                    </v-select>\n                    <v-select\n                      v-model=\"selectedMember2\"\n                      :items=\"member2\"\n                      multiple\n                      chips\n                    >\n                    </v-select>\n                    <v-select\n                      v-model=\"selectedMember3\"\n                      :items=\"member3\"\n                      multiple\n                      chips\n                    >\n                    </v-select>\n                  </v-col>\n    </v-row>\n  </v-container>\n\n           <!-- テスト -->\n          <v-col cols=\"12\">\n            <v-autocomplete\n              v-model=\"friends\"\n              :disabled=\"isUpdating\"\n              :items=\"people\"\n              filled\n              chips\n              color=\"blue-grey lighten-2\"\n              label=\"Select\"\n              item-text=\"name\"\n              item-value=\"name\"\n              multiple\n            >\n              <template v-slot:selection=\"data\">\n                <v-chip\n                  v-bind=\"data.attrs\"\n                  :input-value=\"data.selected\"\n                  close\n                  @click=\"data.select\"\n                  @click:close=\"remove(data.item)\"\n                >\n                  <v-avatar left>\n                    <v-img :src=\"data.item.avatar\"></v-img>\n                  </v-avatar>\n                  {{ data.item.name }}\n                </v-chip>\n              </template>\n              <template v-slot:item=\"data\">\n                <template v-if=\"typeof data.item !== 'object'\">\n                  <v-list-item-content v-text=\"data.item\"></v-list-item-content>\n                </template>\n                <template v-else>\n                  <v-list-item-avatar>\n                    <img :src=\"data.item.avatar\">\n                  </v-list-item-avatar>\n                  <v-list-item-content>\n                    <v-list-item-title v-html=\"data.item.name\"></v-list-item-title>\n                    <v-list-item-subtitle v-html=\"data.item.group\"></v-list-item-subtitle>\n                  </v-list-item-content>\n                </template>\n              </template>\n            </v-autocomplete>\n          </v-col>\n\n          </v-card>\n        </v-dialog>\n      </v-row>\n    </v-sheet>\n  </v-app>\n</template>\n\n\n<style scoped>\n    .my-time-picker >>> .vue__time-picker .dropdown ul li:not([disabled]).active,\n    .my-time-picker >>> .vue__time-picker .dropdown ul li:not([disabled]).active:focus,\n    .my-time-picker >>> .vue__time-picker .dropdown ul li:not([disabled]).active:hover {\n        background: #999;\n    }\n</style>\n\n\n<script>\nimport VueTimepicker from 'vue2-timepicker'\nimport 'vue2-timepicker/dist/VueTimepicker.css'\n\nimport { Datetime } from 'vue-datetime'\nimport 'vue-datetime/dist/vue-datetime.css'\n\n  export default {\n  components: {\n    'vue-timepicker': VueTimepicker,\n    'datetime' : Datetime,\n  },\n    data: () => ({\n      menu:[],\n      menu2: [],\n      dialog: null,\n      dialog2: false,\n      date:'',\n      time:'',\n      datetime:'',\n      notifications: [],\n      drawer: false,\n      group: null,\n      focus: '',\n      type: 'month',\n      typeToLabel: {\n        month: 'Month',\n        week: 'Week',\n        day: 'Day',\n      },\n      selectedEvent: {},\n      selectedElement: null,\n      selectedOpen: false,\n      events: [],\n      colors: ['blue', 'indigo', 'deep-purple', 'cyan', 'green', 'orange', 'grey darken-1'],\n      names: ['Meeting', 'Holiday', 'PTO', 'Travel', 'Event', 'Birthday', 'Conference', 'Party'],\n      states: ['外出',\n                '来社',\n                '会議',\n                'リモート会議',\n                '重要',\n                '会食',\n                '出張',\n                '休憩',\n                '休暇',\n              ],\n      select: { state: 'Florida', abbr: 'FL' },\n      facility: [\n        { state: '3F会議室（ソファー）'},\n        { state: '3F会議室（テーブル）'},\n        { state: '7Fテーブル'},\n        { state: '7F会議室'},\n        { state: 'テスト５'},\n        { state: 'テスト６'},\n      ],\n      member1: [\n        'A子',\n        'B太郎',\n        'Cさん',\n      ],\n      selectedMember1: [],\n      member2: [\n        'Dさん',\n        'Eさん',\n        'Fさん',\n      ],\n      selectedMember2: [],\n      member3: [\n        'Gさん',\n        'Hさん',\n        'Jさん',\n      ],\n      selectedMember3: [],\n    }),\n\n\n\n    mounted () {\n      this.$refs.calendar.checkChange()\n    },\n\n    computed: {\n      likesAllMember1 () {\n        return this.selectedMember1.length === this.member1.length\n      },\n      likesSomeMEmber1 () {\n        return this.selectedMember1.length > 0 && !this.likesAllMember1\n      },\n      icon () {\n        if (this.likesAllMember1) return 'mdi-close-box'\n        if (this.likesSomeMEmber1) return 'mdi-minus-box'\n        return 'mdi-checkbox-blank-outline'\n      },\n      likesAllMember2 () {\n        return this.selectedMember2.length === this.member2.length\n      },\n      likesSomeMEmber2 () {\n        return this.selectedMember2.length > 0 && !this.likesAllMember2\n      },\n      icon () {\n        if (this.likesAllMember2) return 'mdi-close-box'\n        if (this.likesSomeMEmber2) return 'mdi-minus-box'\n        return 'mdi-checkbox-blank-outline'\n      },\n      likesAllMember3 () {\n        return this.selectedMember3.length === this.member3.length\n      },\n      likesSomeMEmber3 () {\n        return this.selectedMember3.length > 0 && !this.likesAllMember3\n      },\n      icon () {\n        if (this.likesAllMember3) return 'mdi-close-box'\n        if (this.likesSomeMEmber3) return 'mdi-minus-box'\n        return 'mdi-checkbox-blank-outline'\n      },\n    },\n\n    methods: {\n      viewDay ({ date }) {\n        this.focus = date\n        this.type = 'day'\n      },\n      getEventColor (event) {\n        return event.color\n      },\n      setToday () {\n        this.focus = ''\n      },\n      prev () {\n        this.$refs.calendar.prev()\n      },\n      next () {\n        this.$refs.calendar.next()\n      },\n      showEvent ({ nativeEvent, event }) {\n        const open = () => {\n          this.selectedEvent = event\n          this.selectedElement = nativeEvent.target\n          requestAnimationFrame(() => requestAnimationFrame(() => this.selectedOpen = true))\n        }\n  \n        if (this.selectedOpen) {\n          this.selectedOpen = false\n          requestAnimationFrame(() => requestAnimationFrame(() => open()))\n        } else {\n          open()\n        }\n  \n        nativeEvent.stopPropagation()\n      },\n      updateRange ({ start, end }) {\n        const events = []\n  \n        const min = new Date(`${start.date}T00:00:00`)\n        const max = new Date(`${end.date}T23:59:59`)\n        const days = (max.getTime() - min.getTime()) / 86400000\n        const eventCount = this.rnd(days, days + 20)\n  \n        for (let i = 0; i < eventCount; i++) {\n          const allDay = this.rnd(0, 3) === 0\n          const firstTimestamp = this.rnd(min.getTime(), max.getTime())\n          const first = new Date(firstTimestamp - (firstTimestamp % 900000))\n          const secondTimestamp = this.rnd(2, allDay ? 288 : 8) * 900000\n          const second = new Date(first.getTime() + secondTimestamp)\n  \n          events.push({\n            name: this.names[this.rnd(0, this.names.length - 1)],\n            start: first,\n            end: second,\n            color: this.colors[this.rnd(0, this.colors.length - 1)],\n            timed: !allDay,\n          })\n        }\n  \n        this.events = events\n      },\n      rnd (a, b) {\n        return Math.floor((b - a + 1) * Math.random()) + a\n      },\n      toggle1 () {\n        this.$nextTick(() => {\n          if (this.likesAllMember1) {\n            this.selectedMember1 = []\n          } else {\n            this.selectedMember1 = this.member1.slice()\n          }\n        })\n      },\n      toggle2 () {\n        this.$nextTick(() => {\n          if (this.likesAllMember2) {\n            this.selectedMember2 = []\n          } else {\n            this.selectedMember2 = this.member2.slice()\n          }\n        })\n      },\n      toggle3 () {\n        this.$nextTick(() => {\n          if (this.likesAllMember3) {\n            this.selectedMember3 = []\n          } else {\n            this.selectedMember3 = this.member3.slice()\n          }\n        })\n      },\n    },\n  }\n</script>\n\n<script>\n  export default {\n    data () {\n      return {\n    