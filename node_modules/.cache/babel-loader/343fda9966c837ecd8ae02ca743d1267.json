con>\n            </v-btn>\n\n            <v-toolbar-title v-if=\"$refs.calendar\">\n              {{ $refs.calendar.title }}\n            </v-toolbar-title>\n\n            <v-btn\n              icon\n              class=\"mr-auto\"\n              @click=\"$refs.calendar.next()\"\n            >\n              <v-icon>mdi-chevron-right</v-icon>\n            </v-btn>\n\n            <v-btn\n              outlined\n              x-small\n              class=\"mr-auto\"\n              @click=\"setToday\"\n            >\n              Today\n            </v-btn>\n      </v-toolbar>\n    </v-sheet>\n\n      <v-navigation-drawer\n        v-model=\"drawer\"\n        absolute\n        left\n        temporary\n      >\n        <v-list\n          nav\n          dense\n        >\n          <v-list-item-group\n            v-model=\"group\"\n            active-class=\"deep-purple--text text--accent-4\"\n          >\n            <h5>　表示の種類</h5>\n            <v-list-item @click=\"type = 'day'\">\n            <v-icon>mdi-view-day-outline</v-icon>\n              <v-list-item-title>　day</v-list-item-title>\n            </v-list-item>\n  \n            <v-list-item @click=\"type = 'week'\">\n            <v-icon>mdi-view-week-outline</v-icon>\n              <v-list-item-title>　week</v-list-item-title>\n            </v-list-item>\n  \n            <v-list-item @click=\"type = 'month'\">\n            <v-icon>mdi-grid</v-icon>\n              <v-list-item-title>　month</v-list-item-title>\n            </v-list-item>\n  \n            <v-list-item>\n            <v-icon>mdi-account-group</v-icon>\n              <v-list-item-title>　gloup</v-list-item-title>\n            </v-list-item>\n  \n            <v-list-item>\n            <v-icon>mdi-account</v-icon>\n              <v-list-item-title>　person</v-list-item-title>\n            </v-list-item>\n\n          </v-list-item-group>\n        </v-list>\n      </v-navigation-drawer>\n\n        <v-sheet height=\"600\">\n          <v-calendar\n            ref=\"calendar\"\n            v-model=\"focus\"\n            color=\"red lighten-2\"\n            :events=\"events\"\n            :event-color=\"getEventColor\"\n            :type=\"type\"\n            @click:event=\"showEvent\"\n            @click:more=\"viewDay\"\n            @click:date=\"viewDay\"\n            @change=\"updateRange\"\n          >\n          </v-calendar>\n        </v-sheet>\n\n  <!-- +アイコンの登録ボタン -->\n  <v-sheet height=\"300\">\n  <v-row justify=\"center\">\n    <v-dialog\n      v-model=\"dialog\"\n      fullscreen\n      hide-overlay\n      transition=\"dialog-bottom-transition\"\n    >\n      <template v-slot:activator=\"{ on, attrs }\">\n\n        <v-btn\n          elevation=\"6\"\n          color=\"grey darken\"\n          fab\n          dark\n          v-bind=\"attrs\"\n          v-on=\"on\"\n        >\n          <v-icon dark>\n            mdi-plus\n          </v-icon>\n        </v-btn>\n\n        <!-- ダイアログ登録フォーム -->\n      </template>\n      <v-card>\n        <v-toolbar\n          dark\n          color=\"grey darken\"\n        >\n          <v-btn\n            icon\n            dark\n            @click=\"dialog = false\"\n          >\n            <v-icon>mdi-close</v-icon>\n          </v-btn>\n          <v-spacer></v-spacer>\n          <v-toolbar-items>\n            <v-btn\n              dark\n              text\n              @click=\"dialog = false\"\n            >\n              Save\n            </v-btn>\n          </v-toolbar-items>\n        </v-toolbar>\n\n        <v-list-item>\n          <v-col\n            cols=\"12\"\n            sm=\"6\"\n            md=\"5\"\n          >\n            <v-text-field\n              label=\"タイトル\"\n            ></v-text-field>\n          </v-col>\n        </v-list-item>\n\n      <template>\n        <v-container fluid>\n          <v-row>\n            <v-col\n              class=\"d-flex\"\n              cols=\"12\"\n              sm=\"3\"\n            >\n              <v-select\n                :items=\"states\"\n                label=\"予定の種類\"\n                outlined\n              ></v-select>\n            </v-col>\n      \n            <v-col\n              class=\"d-flex\"\n              cols=\"12\"\n              sm=\"3\"\n            >\n              <v-select\n                :items=\"items\"\n                item-text=\"state\"\n                item-value=\"abbr\"\n                label=\"設備を選ぶ\"\n                outlined\n              ></v-select>\n            </v-col>\n          </v-row>\n        </v-container>\n      </template>\n\n          <!-- <v-list-item>\n              <v-checkbox v-model=\"notifications\"></v-checkbox>\n              <v-list-item-title >終日</v-list-item-title>\n          </v-list-item> -->\n          \n          <!-- ダイアログ内カレンダー -->\n\n      <template>\n        <v-row>\n          <v-col\n            cols=\"12\"\n            sm=\"6\"\n            md=\"4\"\n          >\n            <v-menu\n              ref=\"menu\"\n              v-model=\"menu\"\n              :close-on-content-click=\"false\"\n              :return-value.sync=\"date\"\n              transition=\"scale-transition\"\n              offset-y\n              min-width=\"auto\"\n            >\n              <template v-slot:activator=\"{ on, attrs }\">\n                <v-text-field\n                  v-model=\"date\"\n                  label=\"Picker in menu\"\n                  prepend-icon=\"mdi-calendar\"\n                  readonly\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                ></v-text-field>\n              </template>\n              <v-date-picker\n                v-model=\"date\"\n                no-title\n                scrollable\n                temporary\n              >\n                <v-spacer></v-spacer>\n                <v-btn\n                  text\n                  color=\"primary\"\n                  @click=\"menu = false\"\n                >\n                  Cancel\n                </v-btn>\n              </v-date-picker>\n            </v-menu>\n          </v-col>\n          <div class=\"my-time-picker\">\n          <vue-timepicker></vue-timepicker>\n          </div>\n        </v-row>\n      </template>\n\n<datetime v-model=\"date\"></datetime>\n\n      </v-card>\n    </v-dialog>\n  </v-row>\n\n    </v-sheet>\n  </v-app>\n</div>\n\n</template>\n\n\n<style scoped>\n    .my-time-picker >>> .vue__time-picker .dropdown ul li:not([disabled]).active,\n    .my-time-picker >>> .vue__time-picker .dropdown ul li:not([disabled]).active:focus,\n    .my-time-picker >>> .vue__time-picker .dropdown ul li:not([disabled]).active:hover {\n        background: #999;\n    }\n</style>\n\n\n<script>\nimport VueTimepicker from 'vue2-timepicker'\nimport 'vue2-timepicker/dist/VueTimepicker.css'\n\nimport { Datetime } from 'vue-datetime'\nimport 'vue-datetime/dist/vue-datetime.css'\n\n  export default {\n  components: {\n    'vue-timepicker': VueTimepicker,\n    'datetime' : Datetime,\n  },\n    data: () => ({\n      menu:[],\n      menu2: [],\n      dialog: null,\n      date:'',\n      time:'',\n      notifications: [],\n      drawer: false,\n      group: null,\n      focus: '',\n      type: 'month',\n      typeToLabel: {\n        month: 'Month',\n        week: 'Week',\n        day: 'Day',\n      },\n      selectedEvent: {},\n      selectedElement: null,\n      selectedOpen: false,\n      events: [],\n      colors: ['blue', 'indigo', 'deep-purple', 'cyan', 'green', 'orange', 'grey darken-1'],\n      names: ['Meeting', 'Holiday', 'PTO', 'Travel', 'Event', 'Birthday', 'Conference', 'Party'],\n      states: ['外出', '来社', '会議', 'リモート会議', '重要', '会食', '出張', '休憩', '休暇', 'test1', 'test2', 'test3', 'test4', 'test5', 'test6', 'test7', 'test8', 'test9', 'test10', 'test11', 'test12'],\n      select: { state: 'Florida', abbr: 'FL' },\n      items: [\n        { state: '3F会議室（ソファー）'},\n        { state: '3F会議室（テーブル）'},\n        { state: '7Fテーブル'},\n        { state: '7F会議室'},\n        { state: 'テスト５'},\n        { state: 'テスト６'},\n        { state: 'テスト７'},\n        { state: 'テスト８'},\n        { state: 'テスト９'},\n        { state: 'テスト10'},\n      ],\n    }),\n\n  watch: {\n    group () {\n      this.drawer = false\n    },\n  },\n\n    mounted () {\n      this.$refs.calendar.checkChange()\n    },\n    methods: {\n      viewDay ({ date }) {\n        this.focus = date\n        this.type = 'day'\n      },\n      getEventColor (event) {\n        return event.color\n      },\n      setToday () {\n        this.focus = ''\n      },\n      prev () {\n        this.$refs.calendar.prev()\n      },\n      next () {\n        this.$refs.calendar.next()\n      },\n      showEvent ({ nativeEvent, event }) {\n        const open = () => {\n          this.selectedEvent = event\n          this.selectedElement = nativeEvent.target\n          requestAnimationFrame(() => requestAnimationFrame(() => this.selectedOpen = true))\n        }\n  \n        if (this.selectedOpen) {\n          this.selectedOpen = false\n          requestAnimationFrame(() => requestAnimationFrame(() => open()))\n        } else {\n          open()\n        }\n  \n        nativeEvent.stopPropagation()\n      },\n      updateRange ({ start, end }) {\n        const events = []\n  \n        const min = new Date(`${start.date}T00:00:00`)\n        const max = new Date(`${end.date}T23:59:59`)\n        const days = (max.getTime() - min.getTime()) / 86400000\n        const eventCount = this.rnd(days, days + 20)\n  \n        for (let i = 0; i < eventCount; i++) {\n          const allDay = this.rnd(0, 3) === 0\n          const firstTimestamp = this.rnd(min.getTime(), max.getTime())\n          const first = new Date(firstTimestamp - (firstTimestamp % 900000))\n          const secondTimestamp = this.rnd(2, allDay ? 288 : 8) * 900000\n          const second = new Date(first.getTime() + secondTimestamp)\n  \n          events.push({\n            name: this.names[this.rnd(0, this.names.length - 1)],\n            start: first,\n            